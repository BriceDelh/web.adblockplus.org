<head>
<style type="text/css">
  html {overflow: auto;}
  html, body, div, iframe {margin: 0px; padding: 0px; height: 100%; border: none;}
  body { background-color: #f8f6f2; background-image: none; }
  .viewcontent { position: absolute; left: 0; top: 0; z-index: 10; width: 100%; height: 100%; overflow-y: auto; overflow-x: hidden; background: #fff}
  .viewcontent > h1, header, footer, #pageactions { display: none; }
  iframe {display: block; width: 100%; border: none; overflow-y: auto; overflow-x: hidden; z-index: 11; }
  #content { margin: 0; height: 0; }
</style>
<link href="https://chrome.google.com/webstore/detail/cfhdojbkjhnklbpkdaibdccddilifddb" rel="chrome-webstore-item">
<link href="https://chrome.google.com/webstore/detail/ldcecbkkoecffmfljeihcmifjjdoepkn" rel="chrome-webstore-item">
<meta content="noindex" name="robots">
</head>
<iframe id="child-frame" seamless="seamless"></iframe>

<script type="text/javascript">
(function()
{
  function setIframe()
  {
    if (window.location.search.substring(0,20) === &quot;?chrome-extension://&quot;)
      document.getElementById(&quot;child-frame&quot;).src = window.location.search.substring(1);
  }
  setIframe();

  function receiveMessage(e)
  {
    if (e.data === &quot;adblockplus-chrome-install&quot;)
    {
      try
      {
        chrome.webstore.install(&quot;https://chrome.google.com/webstore/detail/cfhdojbkjhnklbpkdaibdccddilifddb&quot;, function()
        {
          e.source.postMessage(&quot;adblockplus-chrome-installed&quot;, e.origin);
        });
      }
      catch(err)
      {
        e.source.postMessage(&quot;adblockplus-chrome-install-failed&quot;, e.origin);
      }
    } else if (e.data === &quot;adblockplus-dev-chrome-install&quot;)
    {
      try
      {
        chrome.webstore.install(&quot;https://chrome.google.com/webstore/detail/ldcecbkkoecffmfljeihcmifjjdoepkn&quot;, function()
        {
          e.source.postMessage(&quot;adblockplus-chrome-installed&quot;, e.origin);
        });
      }
      catch(err)
      {
        e.source.postMessage(&quot;adblockplus-chrome-install-failed&quot;, e.origin);
      }
    }
  }
  window.addEventListener(&quot;message&quot;, receiveMessage, false);
}());
</script>
